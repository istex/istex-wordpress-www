{% extends 'base.twig' %}
{% block title %}
  {{ site.name }} - {{category.name}}
{% endblock %}
{% block contenu %}
  <main role="main">

    <h1>{{category.name}}</h1>

    {% for post in posts %}
      <article>
        <h2 id="{{post.id}}">{{post.title}}</h2>
        {{post.content}}
      </article>
    {% endfor %}

  </main>
{% endblock %}
